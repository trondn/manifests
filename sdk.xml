<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <remote name="couchbase" fetch="git://github.com/couchbase/"
          review="review.couchbase.org" />
  <remote name="couchbaselabs" fetch="git://github.com/couchbaselabs/"
          review="review.couchbase.org" />
  <remote name="trondn" fetch="git://github.com/trondn/"
          review="review.couchbase.org" />
  <remote name="avsej" fetch="git://github.com/avsej/"
          review="review.couchbase.org" />
  <default remote="couchbase" revision="master"/>

  <project path="lcb" name="libcouchbase" />
  <project path="node" name="couchnode" />
  <project path="php" name="php-ext-couchbase" />
  <project path="ruby/client" name="couchbase-ruby-client" />
  <project path="ruby/em" name="couchbase-ruby-client-em" />
  <project path="ruby/model" name="couchbase-ruby-model" />
  <project path="demo/beer-sample" name="beer-sample"  remote="avsej"/>
  <project path="demo/beer.rb" name="couchbase-beer.rb"  remote="avsej"/>
  <project path="demo/squish" name="couchbase-squish"  remote="couchbaselabs"/>
  <project path="demo/vacuum" name="vacuum"  remote="trondn"/>
  <project path="demo/vacuum.rb" name="vacuum.rb"  remote="avsej"/>
  <project path="demo/pluginless" name="libev-couchbase-example"  remote="avsej"/>
  <project path="tools" name="sdktools" remote="trondn">
     <copyfile src='Makefile' dest='Makefile'/>
  </project>
</manifest>
